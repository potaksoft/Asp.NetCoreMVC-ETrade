@model List<Employee>

    <div class="display-6">
        Index3
    </div>
    @foreach (Employee emp in Model)
    {
        <div class="lead my-2">
            <h6>@emp.FullName</h6>
            <p>@emp.Id</p>
            <p><strong>Age:</strong>@emp.Age</p>
            <hr />
        </div>
    }